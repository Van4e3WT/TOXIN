{
  "plugins": [
    "fsd"
  ],
  "extends": [
    "airbnb",
    "plugin:fsd/all"
  ],
  "settings": {
    "import/resolver": "webpack"
  },
  "rules": {
    "no-underscore-dangle": "off",
    "class-methods-use-this": "off",
    "no-unneeded-ternary": [
      "error",
      {
        "defaultAssignment": true
      }
    ]
  }
}